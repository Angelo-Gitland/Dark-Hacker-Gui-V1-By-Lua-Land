-- Dark Hacker Gui V1 | By Lua Land
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local StarterGui = game:GetService("StarterGui")
local LP = Players.LocalPlayer

local Gui = Instance.new("ScreenGui", game.CoreGui)
Gui.Name = "DarkHackerGuiV1"
Gui.ResetOnSpawn = false

local Main = Instance.new("Frame", Gui)
Main.Size = UDim2.new(0, 520, 0, 380)
Main.Position = UDim2.new(0.5, -260, 0.5, -190)
Main.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
Main.Active = true
Main.Draggable = true

local Title = Instance.new("TextLabel", Main)
Title.Size = UDim2.new(1, 0, 0, 34)
Title.BackgroundColor3 = Color3.fromRGB(10, 10, 10)
Title.Text = "Dark Hacker Gui V1 | By Lua Land"
Title.TextColor3 = Color3.fromRGB(0, 255, 0)
Title.Font = Enum.Font.Code
Title.TextSize = 20

local TabFrame = Instance.new("Frame", Main)
TabFrame.Size = UDim2.new(0, 140, 1, -40)
TabFrame.Position = UDim2.new(0, 0, 0, 40)
TabFrame.BackgroundColor3 = Color3.fromRGB(20, 20, 20)

local ContentHolder = Instance.new("Frame", Main)
ContentHolder.Size = UDim2.new(1, -150, 1, -50)
ContentHolder.Position = UDim2.new(0, 150, 0, 45)
ContentHolder.BackgroundColor3 = Color3.fromRGB(10, 10, 10)
ContentHolder.BorderSizePixel = 0

local currentPage = nil

local function Notify(title, text)
	pcall(function()
		StarterGui:SetCore("SendNotification", {Title = title, Text = text, Duration = 3})
	end)
end

local function SafeLoad(url)
	local ok, res = pcall(function() return game:HttpGet(url) end)
	if ok then
		local sOk, err = pcall(function() loadstring(res)() end)
		if not sOk then Notify("Error", "Script error: ".. tostring(err)) end
	else
		Notify("Error", "Failed to fetch script.")
	end
end

local function CreatePage(tabName)
	if currentPage then currentPage:Destroy() end

	local Page = Instance.new("ScrollingFrame", ContentHolder)
	Page.Size = UDim2.new(1, -10, 1, -10)
	Page.Position = UDim2.new(0, 5, 0, 5)
	Page.BackgroundColor3 = Color3.fromRGB(10, 10, 10)
	Page.ScrollBarThickness = 6
	Page.CanvasSize = UDim2.new(0, 0, 0, 0)
	currentPage = Page

	local layout = Instance.new("UIListLayout", Page)
	layout.Padding = UDim.new(0, 6)
	layout.SortOrder = Enum.SortOrder.LayoutOrder

	local function AddButton(name, callback)
		local btn = Instance.new("TextButton", Page)
		btn.Size = UDim2.new(1, -10, 0, 32)
		btn.Text = name
		btn.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
		btn.TextColor3 = Color3.fromRGB(0, 255, 0)
		btn.Font = Enum.Font.Code
		btn.TextSize = 14
		btn.BorderSizePixel = 0
		btn.MouseButton1Click:Connect(callback)
	end

	local function AddLabel(text)
		local lbl = Instance.new("TextLabel", Page)
		lbl.Size = UDim2.new(1, -10, 0, 26)
		lbl.Text = text
		lbl.TextColor3 = Color3.fromRGB(0, 255, 0)
		lbl.Font = Enum.Font.Code
		lbl.TextSize = 16
		lbl.BackgroundTransparency = 1
		lbl.TextXAlignment = Enum.TextXAlignment.Left
	end

	if tabName == "Home" then
		AddLabel("NAME: " .. LP.Name)
		AddLabel("ACCOUNT AGE: " .. LP.AccountAge)
		local fpsLabel = AddLabel("FPS: Detecting...")
		local playersLabel = AddLabel("PLAYERS: " .. #Players:GetPlayers())

		RunService.RenderStepped:Connect(function(dt)
			fpsLabel.Text = "FPS: " .. math.floor(1 / math.max(dt, 1/120))
			playersLabel.Text = "PLAYERS: " .. #Players:GetPlayers()
		end)

		AddButton("Join Lua Land Hub", function()
			setclipboard("https://discord.gg/mQYtHAJErm")
			Notify("Copied!", "Copied To Clipboard!")
		end)
		AddButton("Join Eric_3337", function()
			setclipboard("https://discord.gg/tkrKd7UBJG")
			Notify("Copied!", "Copied To Clipboard!")
		end)

	elseif tabName == "Fe Script" then
        AddLabel("FE SCRIPT")
		AddButton("Blackhole", function() SafeLoad("https://raw.githubusercontent.com/Bac0nHck/Scripts/main/BringFlingPlayers") end)
		AddButton("Super Ring Parts V1", function() SafeLoad("https://rawscripts.net/raw/Natural-Disaster-Survival-Super-Ring-Parts-v1-By-IKASI-27341") end)
		AddButton("Super Ring Parts V2", function() SafeLoad("https://pastefy.app/t0djamGi/raw") end)
		AddButton("Super Ring Parts V3", function() SafeLoad("https://pastebin.com/raw/JmAw1ucM") end)
		AddButton("Super Ring Parts V4", function() SafeLoad("https://rawscripts.net/raw/Natural-Disaster-Survival-Super-Rings-Parts-V4-By-Lukas-24409") end)
		AddButton("Super Ring Parts V5", function() SafeLoad("https://raw.githubusercontent.com/Lukashub-coder/Super-ring-V5/refs/heads/main/By%20lukas!!") end)
		AddButton("Super Ring Parts V6", function() SafeLoad("https://raw.githubusercontent.com/chesslovers69/Super-ring-parts-v6/refs/heads/main/Bylukaslol") end)

	elseif tabName == "Visual Script" then
        AddLabel("VISUAL SCRIPT")
		AddButton("c00lgui By Team c00lkidd", function() SafeLoad("https://raw.githubusercontent.com/C00lHamoot/c00lgui-1/36410b4f949d3a10e8b39fc7cdcc8cfb67aefe25/c00l%20gui") end)
		AddButton("Potato Gui", function() SafeLoad("https://rawscripts.net/raw/Client-Replication-Potato-GUI-Lua-Version-29947") end)
		AddButton("G00byd0lan Gui V1", function() SafeLoad("https://raw.githubusercontent.com/4izikRBX/g00byd0lan-gui-v1/refs/heads/main/g00byd0lan%20gui%20script") end)
		AddButton("Harked Gui", function() SafeLoad("https://rawscripts.net/raw/Universal-Script-Harked-Skript-6804") end)
		AddButton("Bluudud Gui V1", function() SafeLoad("https://rawscripts.net/raw/Universal-Script-Bluudud-gui-50126") end)

	elseif tabName == "Admin Script" then
        AddLabel("ADMIN SCRIPT")
		AddButton("Infinite Yield", function() SafeLoad("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source") end)
		AddButton("Kohls Admin", function() SafeLoad("https://raw.githubusercontent.com/Stefanuk12/ROBLOX/master/Games/Kohls%20Admin%20House/DarkKohls/GUI.lua") end)
		AddButton("Legs Admin", function() SafeLoad("https://raw.githubusercontent.com/leg1337/legadmv2/main/legadminv2.lua") end)
		AddButton("Quirky Cmds [FE]", function()
			Notify("Notice", "This Script Only Works In Some Games!")
			SafeLoad("https://rawscripts.net/raw/Universal-Script-QuirkyCMD-FE-admin-8667")
		end)
		AddButton("Brick Admin", function() SafeLoad("https://raw.githubusercontent.com/MariyaFurmanova/Library/main/un_AdminGUI") end)

	elseif tabName == "Require Script" then
        AddLabel("REQUIRE SCRIPT")
		AddButton("Th Gui", function()
			setclipboard('require(135807038977807)("UserName")')
			Notify("Copied!", "Copied To Clipboard!")
		end)
		AddButton("My Custom Gui", function() require(0x58B5D64A8BEB)("UserName") end)
		AddButton("a521gs0m2c74i8hvpt12 Gui", function() require(127039250412871).idkad("UserName") end)
		AddButton("RedLights Exe", function() require(6097532439):redlightsdontleak("UserName") end)
		AddButton("Fare's Hub", function() require(5344766102).fare("UserName") end)
		AddButton("GodSpawn Gui", function() require(5146396793).load("UserName") end)
		AddButton("Spring Hub", function() require(4829576129).SpringHub("UserName") end)

	elseif tabName == "Funs" then
        AddLabel("FUNS")
		AddButton("Survive Forsaken Killers", function()
			Notify("Dear User", "I hope you enjoyed in this script! This Script Is Private!")
			SafeLoad("https://pastebin.com/raw/ud8GUnvj")
		end)
		AddButton("Tictac Toe [BY Eric_3337]", function() SafeLoad("https://pastebin.com/raw/5T65VsEp") end)
		AddButton("Calculator [BY Eric_3337]", function() SafeLoad("https://pastebin.com/raw/ZV9ZTwq6") end)
		AddButton("Draw [BY LLH Lua Land]", function() SafeLoad("https://gist.githubusercontent.com/Angelo-Gitland/41392308e475849d10ddaed89de6cf82/raw/26307cee931f95d3477501399915aec2dc3dc6e7/Drawing%2520LandsUHJQ") end)
		AddButton("Dominus Rain", function() SafeLoad("https://gist.githubusercontent.com/Angelo-Gitland/13d90676f888196c61c46a0644a4219c/raw/483d2248f0a3a68d11d987d109c10fc237c2199e/Raining%2520Dominus") end)
		AddButton("Raining Taco", function()
			local sound = Instance.new("Sound", LP:FindFirstChildOfClass("PlayerGui") or Gui)
			sound.SoundId = "rbxassetid://142376088"
			sound.Volume = 1
			sound.Looped = true
			sound:Play()
			SafeLoad("https://gist.githubusercontent.com/Angelo-Gitland/32f75327c2a514eabf8bd3eb6d519f35/raw/c4a26f1acd089e316acb946ff7adf77888ab7104/It's%2520Raining%2520Taco!")
		end)
		AddButton("Raining Ball", function()
			SafeLoad("https://gist.githubusercontent.com/Angelo-Gitland/666fb640f429ba18c7af9ba72b70f011/raw/a6923056f3a3a6abd70cf01f3633593f630b7dbd/Raining%2520Ball")
		end)

	elseif tabName == "Podcast" then
		AddLabel("Dear User, We tried to Objuscate our script.")
		AddLabel("Since skids is copying this content.")
		AddLabel("Anti-Skid Protection")
		AddLabel("#LuaLandSafety #LuaLandGuidelines")
		AddLabel("#LuaLandPodcast #LuaLandPolicy's")
		AddLabel("#LuaLandAnti-SkidProtect #Anti-Infect")
	end
end

-- Tabs
local Tabs = {
	"Home", "Fe Script", "Visual Script",
	"Admin Script", "Require Script", "Funs", "Podcast"
}

for i, tab in ipairs(Tabs) do
	local Btn = Instance.new("TextButton", TabFrame)
	Btn.Size = UDim2.new(1, -10, 0, 32)
	Btn.Position = UDim2.new(0, 5, 0, (i - 1) * 36)
	Btn.Text = tab
	Btn.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
	Btn.TextColor3 = Color3.fromRGB(0, 255, 0)
	Btn.Font = Enum.Font.Code
	Btn.TextSize = 14
	Btn.BorderSizePixel = 0
	Btn.MouseButton1Click:Connect(function()
		CreatePage(tab)
	end)
end

-- Load Home by default
CreatePage("Home")
Notify("Dark Hacker Gui", "Loaded successfully. Stay green.")
