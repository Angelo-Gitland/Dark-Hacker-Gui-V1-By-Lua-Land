local currentPage = nil

-- Utility: create a Sky (or replace)
local function ApplySky(skyProps)
    -- skyProps is table with keys: SkyboxBk, SkyboxDn, SkyboxFt, SkyboxLf, SkyboxRt, SkyboxUp (values are asset urls or rbxassetid://)
    local sky = Lighting:FindFirstChildOfClass("Sky")
    if not sky then
        sky = Instance.new("Sky")
        sky.Parent = Lighting
    end
    for k,v in pairs(skyProps) do
        if sky[k] then
            sky[k] = v
        end
    end
    Notify("Skybox", "Applied skybox.")
end

-- Utility: spawn a visible part with decal(s)
local function SpawnDecalPart(name, decalId)
    local spawnPos = workspace:FindFirstChild("SpawnLocation") and workspace.SpawnLocation.Position + Vector3.new(0, 10, 0) or (LP.Character and LP.Character:FindFirstChild("HumanoidRootPart") and LP.Character.HumanoidRootPart.Position + (LP.Character.HumanoidRootPart.CFrame.LookVector * 10) + Vector3.new(0,5,0) or Vector3.new(0,10,0))
    local part = Instance.new("Part", workspace)
    part.Name = "LuaLand_DecalPart_"..name
    part.Size = Vector3.new(8,8,1)
    part.Position = spawnPos
    part.Anchored = true
    part.CanCollide = false
    part.Transparency = 0
    local decal = Instance.new("Decal", part)
    decal.Face = Enum.NormalId.Front
    decal.Texture = decalId
    -- Auto-destroy after some time
    delay(60, function()
        pcall(function() if part and part.Parent then part:Destroy() end end)
    end)
    Notify("Decal", "Spawned decal part ("..name.."). It will auto-remove in 60s.")
end

-- Utility: play music (Sound in PlayerGui so it's local)
local function PlayMusic(name, assetId)
    local pg = LP:FindFirstChildOfClass("PlayerGui") or (LP:WaitForChild("PlayerGui",5))
    if not pg then
        Notify("Music", "Failed to find PlayerGui.")
        return
    end
    -- clean previous
    local existing = pg:FindFirstChild("LuaLand_Music_"..name)
    if existing then existing:Destroy() end
    local sound = Instance.new("Sound", pg)
    sound.Name = "LuaLand_Music_"..name
    sound.SoundId = assetId
    sound.Looped = true
    sound.Volume = 1
    sound.PlayOnRemove = false
    sound.RollOffMode = Enum.RollOffMode.Linear
    sound:Play()
    Notify("Music", "Playing "..name)
end

-- SafeLoad helper (keeps your previous behavior)
local function SafeLoad(url)
    local ok, res = pcall(function() return game:HttpGet(url) end)
    if ok then
        local sOk, err = pcall(function() loadstring(res)() end)
        if not sOk then Notify("Error", "Script error: ".. tostring(err)) end
    else
        Notify("Error", "Failed to fetch script.")
    end
end

