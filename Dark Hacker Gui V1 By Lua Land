-- Place this as a LocalScript (StarterPlayerScripts or StarterGui) for best results in exploit environments
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local StarterGui = game:GetService("StarterGui")
local Lighting = game:GetService("Lighting")
local LP = Players.LocalPlayer

-- === Configurable properties (you asked "ADD PROPERTIES HERE") ===
local THEME = {
    MainBg = Color3.fromRGB(15,15,15),
    PanelBg = Color3.fromRGB(10,10,10),
    Accent = Color3.fromRGB(0,255,0),
    TabBg = Color3.fromRGB(20,20,20),
    BtnBg = Color3.fromRGB(25,25,25),
    BtnText = Color3.fromRGB(0,255,0),
    Font = Enum.Font.Code,
    Draggable = true
}
local GUI_NAME = "DarkHackerGuiV1"

-- Helper: safe notify
local function Notify(title, text)
    pcall(function()
        StarterGui:SetCore("SendNotification", {Title = title, Text = text, Duration = 3})
    end)
end

-- Clean existing gui if any
pcall(function()
    local existing = game.CoreGui:FindFirstChild(GUI_NAME)
    if existing then existing:Destroy() end
end)

-- Create GUI
local Gui = Instance.new("ScreenGui")
Gui.Name = GUI_NAME
Gui.ResetOnSpawn = false
Gui.Parent = game.CoreGui

-- Main container
local Main = Instance.new("Frame", Gui)
Main.Size = UDim2.new(0, 520, 0, 420)
Main.Position = UDim2.new(0.5, -260, 0.5, -210)
Main.BackgroundColor3 = THEME.MainBg
Main.Active = true
