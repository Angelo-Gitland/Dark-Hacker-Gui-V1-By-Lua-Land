-- Create Tabs
local tabList = {"Home", "Fe Script", "Visual Script", "Admin Script", "Require Script", "Funs", "Skybox", "Decals", "Musics", "Settings"}

local layoutTabs = Instance.new("UIListLayout", TabFrame)
layoutTabs.Padding = UDim.new(0, 5)
layoutTabs.SortOrder = Enum.SortOrder.LayoutOrder

for _, tabName in ipairs(tabList) do
    local tabButton = Instance.new("TextButton", TabFrame)
    tabButton.Size = UDim2.new(1, -10, 0, 32)
    tabButton.Position = UDim2.new(0, 5, 0, 0)
    tabButton.Text = tabName
    tabButton.Font = THEME.Font
    tabButton.TextSize = 14
    tabButton.BackgroundColor3 = THEME.BtnBg
    tabButton.TextColor3 = THEME.BtnText
    tabButton.BorderSizePixel = 0
    tabButton.MouseButton1Click:Connect(function()
        CreatePage(tabName)
    end)
end

-- Auto-open Home tab
CreatePage("Home")
Notify("Dark Hacker Gui V1", "Successfully Loaded! | By Lua Land ❤️")
