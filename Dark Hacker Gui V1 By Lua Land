            if pg then
                for _, v in ipairs(pg:GetChildren()) do
                    if v:IsA("Sound") and v.Name:match("^LuaLand_Music_") then
                        v:Stop()
                        v:Destroy()
                    end
                end
            end
            Notify("Music", "All music stopped.")
        end)
